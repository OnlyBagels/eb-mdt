		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tsconfig/svelte": "^5.0.4",
		"esbuild": "^0.25.0",
		"typescript": "^5.8.2",
		"vite": "^6.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.1.0",
		"tailwindcss": "^3.4.1",
		"postcss": "^8.4.49",
		"autoprefixer": "^10.4.20"
	},
	"dependencies": {
		"@biomejs/biome": "^1.9.4",
		"@nativewrappers/fivem": "latest",
